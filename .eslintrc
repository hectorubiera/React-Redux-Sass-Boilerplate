{
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js"
                ]
            }
        ],
        "comma-dangle": [
            1,
            {
                "arrays": "never",
                "objects": "never",
                "imports": "never",
                "exports": "never",
                "functions": "ignore"
            }
        ],
        "no-cond-assign": 1,
        "no-console": 1,
        "no-constant-condition": 1,
        "no-control-regex": 1,
        "no-debugger": 1,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty": 1,
        "no-empty-character-class": 1,
        "no-ex-assign": 1,
        "no-extra-boolean-cast": 1,
        "no-extra-parens": 0,
        "no-extra-semi": 1,
        "no-func-assign": 1,
        "no-inner-declarations": [
            1
        ],
        "no-invalid-regexp": 1,
        "no-irregular-whitespace": 1,
        "no-negated-in-lhs": 1,
        "no-obj-calls": 1,
        "no-regex-spaces": 1,
        "no-sparse-arrays": 1,
        "no-unexpected-multiline": 1,
        "no-unreachable": 1,
        "use-isnan": 1,
        "valid-jsdoc": 0,
        "valid-typeof": 1,
        "accessor-pairs": 1,
        "block-scoped-var": 1,
        "complexity": 0,
        "consistent-return": 0,
        "curly": 2,
        "default-case": 1,
        "dot-location": 0,
        "dot-notation": 1,
        "eqeqeq": 1,
        "guard-for-in": 1,
        "no-alert": 1,
        "no-caller": 1,
        "no-case-declarations": 1,
        "no-div-regex": 1,
        "no-else-return": 1,
        "no-empty-pattern": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 1,
        "no-extra-bind": 1,
        "no-fallthrough": 2,
        "no-floating-decimal": 1,
        "no-implicit-coercion": 0,
        "no-implied-eval": 1,
        "no-invalid-this": 0,
        "no-iterator": 1,
        "no-labels": 0,
        "no-lone-blocks": 1,
        "no-loop-func": 2,
        "no-magic-number": 0,
        "no-multi-spaces": 1,
        "no-multi-str": 1,
        "no-native-reassign": 1,
        "no-new-func": 1,
        "no-new-wrappers": 1,
        "no-new": 1,
        "no-octal-escape": 1,
        "no-octal": 1,
        "no-proto": 1,
        "no-redeclare": 2,
        "no-return-assign": 1,
        "no-script-url": 1,
        "no-self-compare": 1,
        "no-sequences": 0,
        "no-throw-literal": 0,
        "no-unused-expressions": 0,
        "no-useless-call": 1,
        "no-useless-concat": 1,
        "no-void": 1,
        "no-warning-comments": 0,
        "no-with": 1,
        "radix": 0,
        "vars-on-top": 0,
        "wrap-iife": 1,
        "yoda": 0,
        "getter-return": 0,
        "no-await-in-loop": 2,
        "for-direction": 2,
        "no-compare-neg-zero": 2,
        "no-global-assign": 2,
        "no-useless-return": 1,
        "no-useless-escape": 1,
        "no-self-assign": 1,
        "strict": 0,
        "init-declarations": 0,
        "no-catch-shadow": 1,
        "no-delete-var": 1,
        "no-label-var": 1,
        "no-shadow-restricted-names": 1,
        "no-shadow": 2,
        "no-undef-init": 1,
        "no-undef": 0,
        "no-undefined": 0,
        "no-unused-vars": [
            1,
            {
                "varsIgnorePattern": "_$|^React$"
            }
        ],
        "no-use-before-define": 1,
        "callback-return": 1,
        "global-require": 0,
        "handle-callback-err": 1,
        "no-mixed-requires": 0,
        "no-new-require": 0,
        "no-path-concat": 1,
        "no-process-exit": 1,
        "no-restricted-modules": 0,
        "no-sync": 0,
        "array-bracket-spacing": 1,
        "block-spacing": [
            1,
            "never"
        ],
        "brace-style": 1,
        "camelcase": 0,
        "comma-spacing": 1,
        "comma-style": 1,
        "computed-property-spacing": [
            1,
            "never"
        ],
        "consistent-this": 0,
        "eol-last": 1,
        "func-names": 0,
        "func-style": 0,
        "id-length": 0,
        "id-match": 0,
        "indent": [
            1,
            4
        ],
        "jsx-quotes": 1,
        "key-spacing": [
            1,
            {
                "afterColon": true
            }
        ],
        "linebreak-style": 1,
        "lines-around-comment": 0,
        "max-depth": 0,
        "max-len": 0,
        "max-nested-callbacks": [
            1,
            3
        ],
        "max-params": [
            1,
            8
        ],
        "max-statements": 0,
        "new-cap": 0,
        "new-parens": 1,
        "newline-after-var": 1,
        "no-array-constructor": 0,
        "no-bitwise": 0,
        "no-continue": 2,
        "no-inline-comments": 1,
        "no-lonely-if": 1,
        "no-mixed-spaces-and-tabs": 1,
        "no-multi-assign": 0,
        "no-multiple-empty-lines": 1,
        "no-negated-condition": 1,
        "no-nested-ternary": 1,
        "no-new-object": 0,
        "no-plusplus": 0,
        "no-restricted-syntax": 0,
        "no-spaced-func": 0,
        "no-ternary": 0,
        "no-trailing-spaces": 1,
        "no-underscore-dangle": 0,
        "no-unneeded-ternary": 0,
        "no-whitespace-before-property": 1,
        "object-curly-spacing": 1,
        "object-property-newline": 1,
        "one-var": 0,
        "operator-assignment": 0,
        "operator-linebreak": 1,
        "padded-blocks": [
            1,
            {
                "blocks": "never",
                "classes": "always",
                "switches": "never"
            }
        ],
        "quote-props": 0,
        "require-jsdoc": 0,
        "semi-spacing": 0,
        "semi": 1,
        "semi-style": [
            1,
            "last"
        ],
        "sort-vars": 0,
        "space-after-keywords": 0,
        "space-before-blocks": 0,
        "space-before-function-paren": [
            1,
            "never"
        ],
        "space-before-keywords": 0,
        "space-in-parens": [
            1,
            "never"
        ],
        "space-infix-ops": 1,
        "space-return-throw-case": 0,
        "space-unary-ops": [
            1,
            {
                "words": true,
                "nonwords": false,
                "overrides": {
                    "new": false,
                    "++": true
                }
            }
        ],
        "spaced-comment": 1,
        "switch-colon-spacing": [
            1,
            {
                "after": false,
                "before": true
            }
        ],
        "wrap-regex": 0,
        "func-call-spacing": 1,
        "keyword-spacing": 1,
        "import/prefer-default-export": 0
    }
}