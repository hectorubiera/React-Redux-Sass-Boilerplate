{
    "name": "webpack-config",
    "version": "1.0.0",
    "description": "Webpack config to use in any project, comes with a dev server",
    "main": "index.js",
    "scripts": {
        "serve": "MODE=development DEVTOOL=false node ./webpack.serve.js",
        "serve:devtool": "MODE=development DEVTOOL=true node ./webpack.serve.js",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "build": "MODE=production webpack --config ./build-utils/webpack.base.js"
    },
    "jest": {
        "setupTestFrameworkScriptFile": "<rootDir>__tests__/setup/setupEnzyme.js",
        "testPathIgnorePatterns": [
            "<rootDir>/__tests__/"
        ],
        "testURL": "http://localhost"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/3in1media/webpack-config.git"
    },
    "keywords": [
        "webpack",
        "config",
        "webpack-serve"
    ],
    "author": "3IN1MEDIA",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/3in1media/webpack-config/issues"
    },
    "homepage": "https://github.com/3in1media/webpack-config#readme",
    "devDependencies": {
        "autoprefixer": "^9.1.1",
        "babel": "^6.23.0",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-eslint": "^8.2.6",
        "babel-loader": "^7.1.5",
        "babel-preset-env": "^1.7.0",
        "babel-preset-es2015-ie": "^6.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "clean-webpack-plugin": "^0.1.19",
        "connect-history-api-fallback": "^1.5.0",
        "css-loader": "^1.0.0",
        "enzyme": "^3.4.1",
        "enzyme-adapter-react-16": "^1.2.0",
        "enzyme-to-json": "^3.3.4",
        "error-overlay-webpack-plugin": "^0.1.5",
        "eslint": "^5.3.0",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-loader": "^2.1.0",
        "eslint-plugin-import": "^2.14.0",
        "eslint-plugin-react": "^7.10.0",
        "eyeglass": "^1.5.0",
        "file-loader": "^1.1.11",
