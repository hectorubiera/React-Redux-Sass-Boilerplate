{
    "name": "react-redux-sass-boilerplate",
    "version": "1.0.0",
    "description": "React Redux SASS Boilerplate to use in any project, comes with a dev server",
    "main": "index.js",
    "scripts": {
        "serve": "MODE=development node ./webpack.serve.js",
        "test": "jest",
        "test:single": "jest -t",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "build": "MODE=production webpack --config ./build-utils/webpack.base.js"
    },
    "jest": {
        "setupTestFrameworkScriptFile": "<rootDir>enzyme.setup.js",
        "snapshotSerializers": [
            "enzyme-to-json/serializer"
        ],
        "testURL": "http://localhost"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hectorubiera/React-Redux-Boilerplate.git"
    },
    "keywords": [
        "webpack",
        "webpack-serve"
    ],
    "author": "hectorubiera",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/hectorubiera/React-Redux-Boilerplate/issues"
    },
    "homepage": "https://github.com/hectorubiera/React-Redux-Boilerplate#readme",
